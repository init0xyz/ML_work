   "source": [
    "import numpy as np\n",
    "a = np.array([1,2]).transpose()\n",
    "b = np.array([3,4])\n",
    "# 按元素相乘\n",
    "print((a.T)*b)\n",
    "# 矩阵相乘\n",
    "print(np.dot(a,b))"
