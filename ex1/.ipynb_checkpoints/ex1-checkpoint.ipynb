    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "data = np.loadtxt(\"ex1data1.txt\",delimiter=',')\n",
    "X = data[:,0]\n",
    "y = data[:,1]\n",
    "X = np.asmatrix(X)\n",
    "y = np.asmatrix(y)\n",
    "plt.plot(X,y,'rx')\n",
    "plt.ylabel(\"Profit in $10,000s\")\n",
    "plt.xlabel(\"Population of City in 10,000s\")"
   ]
